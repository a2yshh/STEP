class FoodOrder {
 String customerName, foodItem;
 int quantity;
 double price;
 FoodOrder() {
 this.customerName = "Unknown";
 this.foodItem = "Unknown";
 }
 FoodOrder(String foodItem) {
 this.foodItem = foodItem;
 this.quantity = 1;
 this.price = 100;
 }
 FoodOrder(String foodItem, int quantity) {
 this.foodItem = foodItem;
 this.quantity = quantity;
 this.price = quantity * 100;
 }
 void printBill() {
 System.out.println("Customer: " + customerName +
 ", Item: " + foodItem +
 ", Quantity: " + quantity +
 ", Total: " + price);
 }
 public static void main(String[] args) {
 FoodOrder f1 = new FoodOrder();
 FoodOrder f2 = new FoodOrder("Burger");
 FoodOrder f3 = new FoodOrder("Pizza", 3);
 f1.printBill();
 f2.printBill();
 f3.printBill();
